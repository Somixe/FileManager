{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "app/**/*",
    "components/**/*",
    "expo-env.d.ts",
    ".expo/types/**/*.js",
    "expo-env.d.js",
    "app/(auth)/signUp.js",
    "app/(auth)/_layout.js",
    "app/(auth)/index.js",
    "app/(auth)/login.js"
  ]
}